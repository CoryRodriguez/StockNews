    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@types/node-fetch": "^2.6.11",
    "@types/ws": "^8.5.10",
