datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  password  String
  createdAt DateTime @default(now())
}

model Watchlist {
  id        String   @id @default(cuid())
  name      String
  tickers   String[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Layout {
  id        String   @id @default(cuid())
  name      String
  panels    Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
